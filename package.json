    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "documentation": "^6.1.0",
    "eslint": "^4.18.2",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^22.4.2",
    "npx": "^10.0.1"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/.jest/polyfills.js",
      "jest-googleapi-mock"
